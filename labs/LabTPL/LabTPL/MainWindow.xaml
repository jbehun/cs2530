        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="Graph Demo" Height="400" Width="675">
    
    <Grid>
        <Image Margin="140,5,5,5" Name="graphImage" Stretch="Fill" />
        <Button Height="23" HorizontalAlignment="Left" Margin="12,38,0,0" Name="plotButton" VerticalAlignment="Top" Width="75" Click="PlotButton_Click">Plot Graph</Button>
        <Label Height="28" HorizontalAlignment="Left" Margin="12,85,0,0" Name="durationLabel" VerticalAlignment="Top" Width="122">Duration (ms):</Label>
    </Grid>
</Window>
